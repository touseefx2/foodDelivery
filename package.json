  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "gclean": "cd android && ./gradlew clean",
    "bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "debug-apk": "npm run gclean && npm run bundle && cd android && ./gradlew assembleDebug",
    "debug": "cd android && ./gradlew assembleDebug",
    "release-apk": "npm run gclean && npm run bundle && cd android && ./gradlew assembleRelease",
    "release": "cd android && ./gradlew assembleRelease",
    "release-bundle": "npm run gclean && npm run bundle && cd android && ./gradlew bundleRelease -x bundleReleaseJsAndAssets",
    "releaseB": " cd android && ./gradlew bundleRelease -x bundleReleaseJsAndAssets",
    "sha1-key": "cd android && ./gradlew signingReport"
  },
